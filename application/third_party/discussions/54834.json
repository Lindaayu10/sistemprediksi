    "Html": "<code>\r\n&lt;?php<br>\r\n$reader = PHPExcel_IOFactory::createReader('Excel2007');<br>\r\n$excel = $reader-&gt;load('test.xlsx');<br>\r\n<br>\r\necho 'Total number of sheets: ' . count($excel-&gt;getAllSheets());<br>\r\n?&gt;<br>\r\n</code>\r\n<br>\r\n",
